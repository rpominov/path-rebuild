{
  "name": "rescript-path-rebuild",
  "version": "0.1.1",
  "description": "Transform a file path by splitting the original path into an array of parts and defining a new path in terms of indices of the parts",
  "main": "./PathRebuild.bs.js",
  "scripts": {
    "build": "rescript",
    "clean": "rescript clean -with-deps",
    "start": "rescript build -w",
    "test": "jest",
    "snap": "jest --updateSnapshot"
  },
  "keywords": [
    "rescript",
    "path",
    "transform",
    "rebuild"
  ],
  "contributors": [
    "Roman Pominov"
  ],
  "homepage": "https://github.com/rpominov/path-rebuild/packages/rescript-path-rebuild#readme",
  "bugs": "https://github.com/rpominov/path-rebuild/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/rpominov/path-rebuild.git",
    "directory": "packages/rescript-path-rebuild"
  },
  "license": "MIT",
  "dependencies": {
    "rescript": "^9.1.4"
  },
  "jest": {
    "testMatch": [
      "**/*.test.bs.js"
    ]
  }
}
